{
  "log": {
    "access": "none",
    "error": "",
    "loglevel": "warning",
    "dnsLog": false
  },
  "-log": {
    "access": "",
    "error": "",
    "loglevel": "debug",
    "dnsLog": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dnsproxy",
        "address": "tcp+local://127.0.0.2",
        "port": 53,
        "queryStrategy": "UseIPv4"
      }
    ],
    "queryStrategy": "UseIPv4",
    "disableCache": false,
    "disableFallback": true,
    "disableFallbackIfMatch": true,
    "useSystemHosts": false,
    "tag": "dns"
  },
  "inbounds": [
    {
      "protocol": "socks",
      "tag": "socks",
      "listen": "127.0.0.1",
      "port": 10801,
      "settings": {
        "udp": true
      }
    },
    {
      "protocol": "http",
      "tag": "http",
      "listen": "127.0.0.1",
      "port": 10802
    },
    {
      "protocol": "vless",
      "tag": "vless",
      "listen": "127.0.0.1",
      "port": 10803,
      "settings": {
        "decryption": "none",
        "clients": [
          {
            "id": "",
            "email": "pox"
          }
        ]
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "",
          "rejectUnknownSni": true,
          "allowInsecure": false,
          "alpn": [
            "h2",
            "http/1.1"
          ],
          "minVersion": "1.3",
          "maxVersion": "1.3",
          "certificates": [
            {
              "oneTimeLoading": true,
              "usage": "encipherment",
              "certificateFile": "/root/lego/.lego/certificates/?.crt",
              "keyFile": "/root/lego/.lego/certificates/?.key"
            }
          ],
          "fingerprint": "firefox"
        },
        "xhttpSettings": {
          "host": "",
          "mode": "auto",
          "path": "/skating/facedown/sip/starless/subpanel/footsie/evasion"
        },
        "realitySettings": {
          "show": false,
          "dest": "",
          "xver": 0,
          "serverNames": [
            ""
          ],
          "privateKey": "",
          "minClientVer": "",
          "maxClientVer": "",
          "maxTimeDiff": 0,
          "shortIds": [
            ""
          ],
          "fingerprint": "chrome",
          "serverName": "",
          "publicKey": "",
          "shortId": "",
          "spiderX": ""
        },
        "rawSettings": {
          "acceptProxyProtocol": false,
          "header": {
            "type": "none|http",
            "request": {
              "version": "1.1",
              "method": "GET",
              "path": [
                "/"
              ],
              "headers": {
                "Host": [
                  "www.baidu.com",
                  "www.bing.com"
                ],
                "User-Agent": [
                  "Mozilla/5.0 (Windows NT 10.0; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/53.0.2785.143 Safari/537.36",
                  "Mozilla/5.0 (iPhone; CPU iPhone OS 10_0_2 like Mac OS X) AppleWebKit/601.1 (KHTML, like Gecko) CriOS/53.0.2785.109 Mobile/14A456 Safari/601.1.46"
                ],
                "Accept-Encoding": [
                  "gzip, deflate"
                ],
                "Connection": [
                  "keep-alive"
                ],
                "Pragma": "no-cache"
              }
            },
            "response": {
              "version": "1.1",
              "status": "200",
              "reason": "OK",
              "headers": {
                "Content-Type": [
                  "application/octet-stream",
                  "video/mpeg"
                ],
                "Transfer-Encoding": [
                  "chunked"
                ],
                "Connection": [
                  "keep-alive"
                ],
                "Pragma": "no-cache"
              }
            }
          }
        },
        "kcpSettings": {
          "//": "https://xtls.github.io/en/config/transports/mkcp.html"
        },
        "grpcSettings": {
          "//": "https://xtls.github.io/en/config/transports/grpc.html",
          "serviceName": "name",
          "multiMode": false,
          "idle_timeout": 60,
          "health_check_timeout": 20,
          "permit_without_stream": false,
          "initial_windows_size": 0
        },
        "wsSettings": {
          "//": "https://xtls.github.io/en/config/transports/websocket.html",
          "acceptProxyProtocol": false,
          "path": "/",
          "host": "xray.com",
          "headers": {
            "key": "value"
          }
        },
        "httpupgradeSettings": {
          "//": "https://xtls.github.io/en/config/transports/httpupgrade.html",
          "acceptProxyProtocol": false,
          "path": "/",
          "host": "xray.com",
          "headers": {
            "key": "value"
          }
        },
        "sockopt": {
          "mark": 0,
          "tcpMaxSeg": 1440,
          "tcpFastOpen": false,
          "tproxy": "off",
          "domainStrategy": "AsIs",
          "happyEyeballs": {
            "tryDelayMs": 250
          },
          "dialerProxy": "",
          "acceptProxyProtocol": false,
          "tcpKeepAliveInterval": 0,
          "tcpKeepAliveIdle": 300,
          "tcpUserTimeout": 10000,
          "tcpCongestion": "bbr",
          "interface": "wg0",
          "v6only": false,
          "tcpWindowClamp": 600,
          "tcpMptcp": false,
          "tcpNoDelay": false
        }
      }
    },
    {
      "protocol": "shadowsocks",
      "tag": "shadowsocks",
      "listen": "127.0.0.1",
      "port": 10803,
      "settings": {
        "//": "https://xtls.github.io/en/config/inbounds/shadowsocks.html#clientobject",
        "clients": [
          {
            "password": "",
            "method": "aes-256-gcm",
            "level": 0,
            "email": "love@xray.com"
          }
        ],
        "network": "tcp,udp"
      }
    },
    {
      "protocol": "vmess",
      "tag": "vmess",
      "listen": "127.0.0.1",
      "port": 10803,
      "settings": {
        "clients": [
          {
            "id": "",
            "level": 0,
            "email": "love@xray.com"
          }
        ]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "freedom",
      "streamSettings": {
        "sockopt": {
          "interface": "warp"
        }
      }
    },
    {
      "protocol": "freedom",
      "tag": "dnsproxy",
      "settings": {
        "redirect": "127.0.0.2:53"
      }
    },
    {
      "protocol": "blackhole",
      "tag": "blackhole"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "ip": [
          "9.9.9.9/32",
          "9.9.9.11/32"
        ],
        "port": "53",
        "network": "udp",
        "outboundTag": "dnsproxy"
      },
      {
        "type": "field",
        "domain": [
          "geosite:private",
          "geosite:category-ir",
          "geosite:category-ads-all",
          "geosite:category-public-tracker"
        ],
        "outboundTag": "blackhole"
      },
      {
        "type": "field",
        "ip": [
          "geoip:private",
          "geoip:ir"
        ],
        "outboundTag": "blackhole"
      }
    ]
  }
}
